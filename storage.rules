
rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /payment-attachments/{userId}/{fileName} {
      allow read, write: if request.auth != null && request.auth.uid == userId;
    }
    match /incident-attachments/{userId}/{fileName} {
      allow read, write: if request.auth != null && request.auth.uid == userId;
    }
  }
}
